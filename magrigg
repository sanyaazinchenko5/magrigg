  require([
    'jquery'
  ], function (jQuery) {
    if(jQuery(".paypal-review-discount.cart-summary").length){
      jQuery("body").on("click", ".paypal-review-discount.cart-summary .giftcard .title", function(){
        jQuery(".paypal-review-discount.cart-summary .giftcard .content").toggleClass("show");
      })
    }




    jQuery("body").on("click", ".megaMenu", function(){
      if(jQuery(window).width() < 1024){
        var height = jQuery(".page-header").height();
        var windowheight = window.innerHeight;
        jQuery(".navigation_full-wrapper").css({"top": height, "height": windowheight - height});
      }
    })
    if (jQuery("#search.input-text").length){

      var left = jQuery("#search_mini_form").offset().left;
      var search_top = jQuery("#search_mini_form").offset().top + jQuery("#search_mini_form").height();

      if (jQuery("#klevu-pt-rs-hover.klevu-pt-rs-hover").length){
        jQuery("#klevu-pt-rs-hover.klevu-pt-rs-hover").css('left', left);
        jQuery("#klevu-pt-rs-hover.klevu-pt-rs-hover").css('marginTop', search_top);
      }

      if (jQuery("#klevuSearchingArea.klevu-searching-area-l2").length){
