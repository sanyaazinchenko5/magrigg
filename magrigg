        jQuery("#loaderContainer.klevu-searching-area-l2").css('left', left);
        jQuery("#loaderContainer.klevu-searching-area-l2").css('marginTop', search_top);
      }
    }



    document.addEventListener("DOMContentLoaded", function(event) {
      if(jQuery(".checkout-cart-index").length){
        var clickCounter = 0;
        var clickInterval = setInterval(function(){
          // do your thing
          if(jQuery(".field.choice.item").length) {
            jQuery("#co-shipping-method-form").find(".field.choice.item:first-of-type").find("input[type='radio']:last-of-type").click();

            if (!jQuery(".block.shipping").hasClass("active")) {
              jQuery(".block.shipping").find(".title").click();
            }
            clearInterval(clickInterval);
          }

          clickCounter++;
          console.log("click");
          if(clickCounter === 40) {
            clearInterval(clickInterval);
          }
        }, 200);
      }
    });
  });


